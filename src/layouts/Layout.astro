---
import Scripts from "../components/Scripts.astro";
import Seo from "../components/Seo.astro";
import "../css/index.css";

const {
  title,
  description,
  image,
  url = Astro.url.href,
  typeContent,
} = Astro.props;

console.log(image);
---

<!doctype html>
<html lang="en">
  <head>
    <Seo
      title={title}
      description={description}
      typeContent={typeContent}
      image={image}
      url={url}
    />
    <Scripts />
  </head>
  <body>
    <header style="padding-bottom: 5rem;"></header>
    <slot />
  </body>
</html>
